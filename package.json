{
  "homepage": "https://cs-internship.github.io/CS-Queue-Calendar/",
  "name": "cs-queue-calendar",
  "version": "1.0.1",
  "description": "React calendar for displaying CS Internship queue meetings with Persian/Gregorian support.",
  "main": "./src/index.jsx",
  "scripts": {
    "start": "cross-env HOST=0.0.0.0 react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "react-scripts build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "react-scripts test --watchAll=false --coverage",
    "lint": "eslint src --ext .js,.jsx --format stylish --max-warnings=0",
    "lint:fix": "eslint src --ext .js,.jsx --format stylish --max-warnings=0 --fix",
    "act": "act --env-file .env.act --quiet",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cs-internship/CS-Queue-Calendar.git"
  },
  "keywords": [
    "calendar",
    "react",
    "jalali",
    "persian",
    "cs-internship",
    "queue",
    "schedule"
  ],
  "author": {
    "name": "Ali Sadeghi",
    "url": "https://github.com/Ali-Sdg90"
  },
  "contributors": [
    {
      "name": "Ali Sadeghi",
      "url": "https://github.com/Ali-Sdg90"
    }
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "dependencies": {
    "antd": "^5.23.0",
    "crypto-js": "^4.2.0",
    "gh-pages": "^6.3.0",
    "jalali-moment": "^3.3.11",
    "moment": "^2.30.1",
    "moment-jalaali": "^0.10.4",
    "moment-timezone": "^0.5.46",
    "react": "^18.3.1",
    "react-big-calendar": "^1.18.0",
    "@ant-design/icons": "^5.4.0",
    "dayjs": "^1.11.9",
    "react-dom": "^18.3.1",
    "react-scripts": "^5.0.1",
    "react-toastify": "^11.0.3",
    "sass": "^1.83.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@commitlint/cli": "^17.6.0",
    "@commitlint/config-conventional": "^17.6.0",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-refresh": "^0.4.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "babel-jest": "^29.7.0",
    "prettier": "^3.6.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "publishConfig": {
    "branch": "gh-pages"
  }
}
